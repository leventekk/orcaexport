{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    {
      "identifier": "fs:allow-config-read-recursive",
      "allow": [
        {
          "path": "$CONFIG/OrcaSlicer/**"
        }
      ]
    },
    "fs:allow-desktop-write"
  ]
}