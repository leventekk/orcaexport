{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "OrcaExport",
  "version": "0.1.0",
  "identifier": "com.orcaexport.app",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "OrcaExport",
        "resizable": false,
        "center": true,
        "dragDropEnabled": false,
        "maximizable": false,
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app", "msi"],
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
